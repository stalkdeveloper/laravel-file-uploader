{
    "name": "stalkdeveloper/laravel-file-uploader",
    "description": "A Laravel package for handling file uploads from both file and URL, saving to Laravel public storage.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/filesystem": "^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StalkArtisan\\LaravelFileUploader\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sunny Kumar",
            "email": "sunnyk.kongu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "StalkArtisan\\LaravelFileUploader\\Providers\\FileUploaderServiceProvider"
            ],
            "aliases": {
                "FileUploader": "StalkArtisan\\LaravelFileUploader\\Facades\\FileUploader"
            }
        }
    },
    "homepage": "https://github.com/stalkdeveloper/laravel-file-uploader",
    "keywords": ["laravel", "file upload", "uploader", "storage"],
    "support": {
        "issues": "https://github.com/stalkdeveloper/laravel-file-uploader/issues",
        "source": "https://github.com/stalkdeveloper/laravel-file-uploader"
    }
}